    "babel-preset-es2015": "^6.14.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "co-mocha": "^1.1.3",
    "genomatic": "^1.0.0",
    "geth-private": "^1.5.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.15.0",
    "mocha": "^3.0.2",
